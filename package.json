{
  "name": "no-mans-connect",
  "productName": "NoMansConnect",
  "version": "0.16.0",
  "description": "A location manager for No Man's Sky.",
  "main": "main.js",
  "scripts": {
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=16192 --trace-warnings -r babel-register -r babel-polyfill  ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./webpack.config.development.js",
    "build-main": "cross-env NODE_ENV=production node --max_old_space_size=16192 --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node --max_old_space_size=16192 --trace-warnings -r babel-register -r babel-polyfill ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron --js-flags=\"--max_old_space_size=16192\" ./app/",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron --inspect=5858 --max_old_space_size=16192 -r babel-register -r babel-polyfill ./app/main.development",
    "postinstall": "concurrently \"install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "dev": "cross-env START_HOT=1 npm run hot-server",
    "nsis": "build --win --x64",
    "pack-win": "build --publish never --dir",
    "build-linux": "build --linux",
    "build-win": "build --win --x64",
    "package": "npm run build && build --publish never",
    "package-win": "npm run build && build --win --x64",
    "package-linux": "npm run build && build --linux",
    "package-all": "npm run build && build -mwl",
    "cleanup": "mop -v"
  },
  "browserslist": "electron 1.7",
  "build": {
    "productName": "NoMansConnect",
    "appId": "org.jaszhix.nmsc",
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "license": "LICENSE.md",
      "deleteAppDataOnUninstall": true
    },
    "asar": true,
    "files": [
      "dist/",
      "node_modules/axios/*",
      "node_modules/lodash/*",
      "app.html",
      "main.js",
      "main.js.map",
      "package.json",
      "ajaxWorker.js",
      "formatWorker.js",
      "jsonWorker.js",
      "mapWorker.js",
      "json.js",
      "each.js",
      "copy.js"
    ],
    "extraFiles": [
      {
        "from": "./app/nmssavetool",
        "to": "./nmssavetool"
      },
      {
        "from": "./app/textures",
        "to": "./app/textures"
      }
    ],
    "extraResources": [
      {
        "from": "node_modules/axios",
        "to": "node_modules/axios"
      },
      {
        "from": "node_modules/lodash",
        "to": "node_modules/lodash"
      },
      {
        "from": "node_modules/is-buffer",
        "to": "node_modules/is-buffer"
      }
    ],
    "dmg": {
      "icon": "./resources/icons/256x256.icns",
      "iconSize": 256,
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "category": "Game",
      "description": "A location manager for No Man's Sky.",
      "target": [
        "deb",
        "pacman",
        "rpm"
      ],
      "desktop": {
        "type": "Application",
        "encoding": "UTF-8",
        "comment": "A location manager for No Man's Sky.",
        "exec": "nomansconnect",
        "icon": "./resources/icons/48x48.png"
      },
      "executableName": "nomansconnect"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaszhix/NoMansConnect.git"
  },
  "author": {
    "name": "Jason Hicks",
    "email": "jaszhix@gmail.com",
    "url": "https://github.com/jaszhix"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaszhix/NoMansConnect/issues"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "react-router",
    "flux",
    "webpack",
    "react-hot"
  ],
  "homepage": "https://github.com/jaszhix/NoMansConnect#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/node_modules"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "devDependencies": {
    "asar": "^0.13.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-remove-console": "^6.8.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "boiler-room-custodian": "^0.6.2",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.4",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.4",
    "devtron": "^1.4.0",
    "electron": "1.7.5",
    "electron-builder": "19.21.0",
    "electron-devtools-installer": "^2.2.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.4.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "expose-loader": "^0.7.3",
    "express": "^4.15.3",
    "fbjs-scripts": "^0.8.1",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jsdom": "^11.1.0",
    "minimist": "^1.2.0",
    "node-loader": "^0.6.0",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "redux-logger": "^3.0.6",
    "sinon": "^3.0.0",
    "spectron": "^3.7.2",
    "style-loader": "^0.18.2",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony-v2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "webpack": "3.5.3",
    "webpack-dev-server": "2.7.1",
    "webpack-merge": "^4.1.0",
    "worker-loader": "^0.8.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "curlrequest": "^1.0.1",
    "dotenv": "^4.0.0",
    "electron-debug": "^1.4.0",
    "electron-machine-id": "^2.0.0",
    "electron-window-state": "^4.1.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "font-awesome": "^4.7.0",
    "fuse.js": "^3.0.5",
    "graceful-fs": "^4.1.11",
    "lodash": "^4.17.4",
    "mathjs": "3.16.1",
    "moment": "^2.18.1",
    "node-localstorage": "^1.3.0",
    "node-machine-id": "^1.1.9",
    "object-sizeof": "^1.2.0",
    "open-external": "^0.1.1",
    "prop-types": "^15.5.10",
    "raven-js": "^3.17.0",
    "react": "16.0.0-beta.5",
    "react-autobind": "^1.0.6",
    "react-dom": "16.0.0-beta.5",
    "react-hot-loader": "3.0.0-beta.7",
    "react-markdown": "^2.5.0",
    "react-mixin": "^3.0.5",
    "react-onclickoutside": "https://github.com/Andarist/react-onclickoutside.git",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-tooltip": "^3.3.0",
    "react-utils": "^1.0.0",
    "react-visibility-sensor": "^3.10.1",
    "recharts": "^1.0.0-alpha.3",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reflux": "^6.4.1",
    "resolve-dir": "^1.0.1",
    "rng-js": "^1.0.1",
    "source-map-support": "^0.4.15",
    "threact": "^0.4.3",
    "three": "^0.86.0",
    "tween.js": "^16.6.0",
    "vquery": "^5.0.1",
    "watch": "^1.0.2",
    "win-ps": "0.0.1"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
